<div id="page_wrapper">
  <h1>Edit Post</h1>

  <%= render 'form' %>
<%= form_for(@post, url: post_path(@post), method: :patch) do |form| %>
  <!-- Other form fields -->
  <div class="field">
    <%= form.label :online %>
    <%= form.check_box :online %>
  </div>
  
  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
</div>
